super + b
    firefox

super + F1
    	pactl set-sink-mute @DEFAULT_SINK@ toggle

super + F2
    	pactl set-sink-volume @DEFAULT_SINK@ -5%

super + F3
    	pactl set-sink-volume @DEFAULT_SINK@ +5%

super + F5
	brightnessctl set 5%-

super + F6
	brightnessctl set +5%

super + Return
	alacritty

super + r
	rofi -show drun

super + w
	alacritty -e nmtui

super + shift + w
	nitrogen --set-zoom-fill --random ~/.config/wallpapers/	
	
